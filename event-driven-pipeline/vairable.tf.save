variable "region" {
  description = "AWS region"
  type = string
  default = "us-east-1"
}

variable  "edp-raw-bucket-archana-devops05" {
  description = "Unique name for raw data S3 bucket"
  type = string
  default = "edp-raw-bucket-unique-12345" # CHANGE to unique name for your account
}

variable "edp-processed-bucket-archana-devops05""edp-processed-bucket-archana-devops05"vvvv"edp-processed-bucket-archana-devops05"vo {
  description = "Unique name for processed data S3 bucket"
  type = string
  default = "edp-processed-bucket-unique-12345" # CHANGE to unique name
}

variable "processor_zip_path" {
  description = "Relative path to processor lambda zip"
  type = string
  default = "../lambda_zip/processor.zip"
}

variable "report_zip_path" {
  description = "Relative path to report lambda zip"
  type = string
  default = "../lambda_zip/report.zip"
}
